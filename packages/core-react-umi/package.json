{
  "private": true,
  "name": "core-react-umi",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.38.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "umi": "^3.5.34"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@umijs/plugin-qiankun": "^2.44.1",
    "@umijs/preset-react": "~1.8.32",
    "@umijs/test": "^3.5.41",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "yorkie": "^2.0.0"
  }
}
